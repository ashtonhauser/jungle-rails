<h1>Signup:</h1>

<%= form_for @user do |form| %>
  <%= form.label :first_name %>:
  <%= form.text_field :first_name%><br />
  <%= form.label :last_name %>:
  <%= form.text_field :last_name%><br />
  <%= form.label :email %>:
  <%= form.text_field :email %><br />
  <%= form.label :password %>:
  <%= form.password_field :password %><br />
  <%= form.submit "Submit" %>
<% end %>
